{
  "name": "yagoo-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for YAGOO - The Agent Guide for the Internet. Enables AI agents to discover and recommend other agents.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "yagoo-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai-agents",
    "directory",
    "yagoo",
    "agent-discovery",
    "claude",
    "llm",
    "ai-tools"
  ],
  "author": "YAGOO",
  "license": "MIT",
  "homepage": "https://yagoo.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/kanoliban/yagoo-mcp-server"
  },
  "bugs": {
    "url": "https://github.com/kanoliban/yagoo-mcp-server/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
